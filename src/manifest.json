{
  "name": "WhereWasI",
  "description": "todo",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "batch-icon128.png",
    "48": "batch-icon48.png",
    "16": "batch-icon16.png"
  },
  "permissions": [
      "webNavigation","tabs","*://*/*"
  ],
  "background": {
    "scripts": ["common.js","background.js"]
  },
  "content_scripts": [
  {
    "matches": ["*://*/*"],
    "js": ["common.js","content.js"]
  }
  ]
}