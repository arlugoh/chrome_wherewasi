{
  "name": "WhereWasI",
  "description": "todo",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "mysteryicon.png"
  },
  "permissions": [
      "webNavigation","tabs","*://*/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
  {
    "matches": ["*://*/*"],
    "js": ["content.js"]
  }
  ]
}